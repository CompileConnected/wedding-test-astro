{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key":"Permissions-Policy",
          "value": "geolocation=(), microphone=(), camera=(),fullscreen=(self \"https://www.youtube.com\")"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "X-DNS-Prefetch-Control",
          "value": "on"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'none'; connect-src 'self' https://vitals.vercel-analytics.com/v1/vitals; font-src 'self' https://fonts.googleapis.com https://fonts.gstatic.com; frame-src 'self' https://www.youtube-nocookie.com; img-src 'self' https://via.placeholder.com "
        }
      ]
    }
  ]
}