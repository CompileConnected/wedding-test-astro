{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key":"Permissions-Policy",
          "value": "geolocation=(), microphone=(), camera=(),fullscreen=(self \"https://www.youtube.com\")"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "X-DNS-Prefetch-Control",
          "value": "on"
        },
        {
          "key": "Content-Security-Policy",
          "value": "object-src 'none', frame-src 'self' youtube.com www.youtube.com youtube-nocookie.com www.youtube-nocookie.com; "
        }
      ]
    }
  ]
}